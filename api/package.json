{
  "name": "backcountry-data-api",
  "version": "0.0.1",
  "description": "An API for accessing SNOTEL data in JSON format from the current water year. Data is cached and fetched every 15 minutes.",
  "keywords": [
    "snotel",
    "weather",
    "skiing",
    "snow"
  ],
  "author": {
    "name": "Robby Milo",
    "email": "robbymilo@gmail.com"
  },
  "homepage": "https://backcountrydata.com",
  "private": false,
  "scripts": {
    "start": "node app.js",
    "prettier": "prettier --check '**/*.js' --write"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "csvtojson": "^2.0.8",
    "debug": "~2.6.9",
    "express": "^4.16.4",
    "geolib": "^2.0.24",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.1",
    "node-html-parser": "^1.1.11",
    "prettier": "^2.1.2",
    "xml2json": "^0.11.2"
  },
  "engines": {
    "node": "14.14.0"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "bracketSpacing": false,
    "arrowParens": "always"
  }
}
